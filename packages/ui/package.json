    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "prefixes": "npm rebuild node-sass",
    "build": "npm run prefixes && npx webpack --config webpack.config.js",
    "start:dev": "npm run prefixes && ./node_modules/.bin/webpack-dev-server --port 8080 --host 0.0.0.0",
    "storybook:build": "npm run prefixes && build-storybook -c .storybook -o .storybook-dist",
    "storybook:dev": "npm run prefixes && start-storybook --quiet -p 9009",
    "lint": "npm run prefixes && eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:unit && npm run test:service",
    "test:service": "jest --config jest.config.service.json",
    "test:unit": "jest --config jest.config.unit.json",
    "test:visual-regression": "npx chromatic --exit-zero-on-changes --build-script-name storybook:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliehamilton3/popstack.git"
  },
  "author": "Ellie Hamilton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elliehamilton3/popstack/issues"
  },
  "homepage": "https://github.com/elliehamilton3/popstack#readme",
  "devDependencies": {
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.21",
    "@types/jest": "^26.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chromatic": "^5.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.1.0",
    "jest": "^26.1.0",
    "jquery": "^3.5.1",
    "script-loader": "^0.7.2",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7",
    "webpack-dev-server": "^3.11.0"
  }
