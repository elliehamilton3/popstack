    "normalize.css": "^8.0.1",
    "pathseg": "^1.2.0",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-hooks-global-state": "^1.0.1",
    "react-pdf": "^5.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.0",
